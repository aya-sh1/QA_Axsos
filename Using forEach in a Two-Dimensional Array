public class equal {
	public static void main(String[] args) {
		int number[][]= {{1,2,3,4,5,2,4},{6,8,10,3,6,9,12,15}};
		
		int summation=0;
		
		for(int[] test1:number) {
		for (int test2:test1) {
			System.out.println("Value is: "+test2);
			summation+=test2;
		
			}
		}System.out.println("summation: "+summation);
	}			

}
